@model IEnumerable<LanguageFeatures.Models.Product>


<table>
    <thead>
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Price</th>
            <th>Description</th>
        </tr>
    </thead>

    <tbody>
        @foreach(var item in Model) {
            <tr>
                <td>@item.ProductId</td>
                <td>@item.Name</td>
                <td>@item.Price</td>
                <td>@item.Description</td>
            </tr>
        }

    </tbody>



</table>
